<view>
  <image src='http://i1.fuimg.com/650899/7f42bfde6cb6e992.png' class='nav-img' bindtap='test'></image>
  <view class='list'>
    <view class='name' bindtap='editData'>
      <view>{{username}}</view>
      <image src='http://i1.fuimg.com/650899/cc48c4e96fb39538.png' class='img'></image>
    </view>
    <view >
      <view class='classification'>
        <image src='http://i1.fuimg.com/650899/0863b1b6db46cc1a.png' class='img2'></image>
        <text>VIP会员</text>
        <image src='http://i1.fuimg.com/650899/03d16662c60e221f.png' class='img3'></image>
        <text>积分: 0</text>
      </view>
      <view class='classification2'>
        <button open-type='contact' class='services'>
          <image src='http://i1.fuimg.com/650899/6cbb31d6c87377c7.png' class='img5'></image>
          <text>私人秘书</text>
        </button>
      </view>
      <view class='QRcode'>
        <image src='http://i1.fuimg.com/650899/c85bf99edb5efeb6.png' class='img4'></image>
      </view>
    </view>
  </view>
  <view class='list2'>
    <view class='project' bindtap='toOrderList'>
      <image src='http://i1.fuimg.com/650899/124c8815f2359c20.png' class='project-img'></image>
      <view class='box'>我的交易</view>
      <view class='line'></view>
    </view>
    <view class='project' bindtap='toMyAuction'>
      <image src='http://i1.fuimg.com/650899/0035620fd0d31e75.png' class='project-img'></image>
      <view class='box'>我的拍卖</view>
      <view class='line'></view>
    </view>
    <view class='project' bindtap='toMyCoupon'>
      <image src='http://i1.fuimg.com/650899/1157dd01ba5b6461.png' class='project-img'></image>
      <view class='box'>我的代金券</view>
    </view>
    <view class='line2'></view>
    <view class='project'>
      <image src='http://i1.fuimg.com/650899/77b22442fd6101c5.png' class='project-img'></image>
      <view class='box'>口袋御翠府</view>
      <view class='line'></view>
    </view>
    <view class='project'>
      <image src='http://i1.fuimg.com/650899/e293cfe68d8a5bfd.png' class='project-img'></image>
      <view class='box'>购物车</view>
      <view class='line'></view>
    </view>
    <view class='project' bindtap='toMyCollection'>
      <image src='http://i1.fuimg.com/650899/47ae96f371038217.png' class='project-img'></image>
      <view class='box'>我的收藏</view>
    </view>
  </view>
</view>